{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "css": ["content.css"],
            "js": ["jquery.js", "content.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "http://localhost:3000/",
        "http://localhost/",
        "storage"
        
    ],
    "web_accessible_resources": ["spinner.gif"]
}
